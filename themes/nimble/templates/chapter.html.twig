{% extends 'docs.html.twig' %}

{% block content %}
<div class="col-sm-9 col-sm-offset-3 col-md-9 col-md-offset-3 main">
	<div id="chapter">
    	<div id="body-inner">
			<p>
				{{ page.content }}
			</p>
		</div>
		{% block footer %}
		<blockquote id="github-contrib">
		    <i class="fa fa-github"></i> Found errors? Think you can improve this documentation? Please <a href="{{ site.github.tree ~  ('/'~page.filePathClean)|replace({'/user/':''}) }}">edit this page</a>. 
		</blockquote>
		{% endblock %}
    </div>
</div>
{% endblock %}

